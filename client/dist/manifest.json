{
    "manifest_version": 2,
    "short_name": "CSSEd.it",
    "name": "LiveCSSEd.it",
    "version": "1.14",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "CSSEd.it"
    },
    "description": "Have fun editing the CSS of any website with CSSED.it",
    "icons": {
        "64": "icon_64.png",
        "128": "icon_128.png"
    },
    "homepage_url": "https://github.com/zachystuff/2020-04-04-Live-CSS-Editor"
}
